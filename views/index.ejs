<body class="u-body">

    <section class="u-align-center u-clearfix u-grey-5 u-section-2" id="carousel_d6b1">
        <div class="u-clearfix u-sheet u-valign-middle-xs u-sheet-1">
            <div class="u-align-left u-container-style u-expanded-width-xs u-group u-white u-group-1">
                <div class="u-container-layout u-container-layout-1">
                    <h4 class="u-custom-font u-font-raleway u-text u-text-1">Your to do list</h4>
                    <p class="u-custom-font u-font-raleway u-text u-text-2"><%= listType %></p>
                    <div class="u-border-17 u-border-palette-2-base u-line u-line-horizontal u-line-1"></div>
                    <ul class="u-custom-list u-spacing-10 u-text u-text-default u-text-2">
                        <li>
                            <% itemList.forEach((item)=> { %>
                                <p>
                                    <form action="/delete" method="post">
                                        <input type="checkbox" name="myCheckBox" id="packers" value=<%= item._id %> onChange="this.form.submit()">
                                        <label for="packers" class="strikethrough"><%= item.title %></label>
                                        <input type="hidden" name="listName" value="<%= listType %>">
                                    </form>
                                </p>
                            <% }) %>
                            <br>
                        </li>
                    </ul>
                    <form style="margin-bottom: 10px;" action="/" method="post">
                        <input class="top extended-input" type="text" name="newItem" id="" placeholder="Enter the new item" autofocus autocomplete="off">
                        <button class="btn btn-dark shifted-button btn-lg" type="submit" name="listType" value = <%= listType %>>+</button>
                    </form>
                </div>
            </div>
        </div>
    </section>
